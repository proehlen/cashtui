{
  "name": "my-bitcoin-cash-cli",
  "version": "0.0.1",
  "description": "CLI for my-bitcoin-cash-lib",
  "main": "bin/index.js",
  "scripts": {
    "axios": "^0.18.0",
    "build": "babel src -d bin",
    "clean": "rimraf bin",
    "eslint": "eslint src",
    "flow": "flow",
    "flowbuild": "flow-copy-source src bin",
    "postinstall": "npm run build",
    "prebuild": "npm run eslint && npm run clean && npm run flowbuild",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run build && node bin/index",
    "start": "node bin/index.js"
  },
  "author": "Peter Roehlen",
  "license": "MIT",
  "dependencies": {
    "args": "^5.0.0",
    "clear-console": "^1.1.0",
    "cliui": "^4.1.0",
    "colors": "^1.3.2",
    "my-bitcoin-cash-lib": "file:../my-bitcoin-cash-lib"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "flow-copy-source": "^2.0.2",
    "rimraf": "^2.6.2"
  }
