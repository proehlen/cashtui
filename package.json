{
  "name": "cashtui",
  "version": "0.0.15",
  "description": "Text UI for Bitcoin Cash nodes",
  "homepage": "https://github.com/proehlen/cashtui#README.md",
  "main": "dist/index.js",
  "author": "Peter Roehlen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/proehlen/cashtui.git"
  },
  "bin": {
    "cashtui": "./bin/cashtui.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "keywords": [
    "bitcoin",
    "cash",
    "bch",
    "ui",
    "developer",
    "tool",
    "user interface",
    "command line",
